@model Menu.Models.Reporte6_general

<form>
    <div class="panel panel-default" style="border-color:darkblue; margin-bottom:0px; padding-bottom:0em; margin-top:0px; padding-top:0em;">
        <div class="panel-body">
            <table class="table" style="border-color:darkblue; margin-bottom:0px; padding-bottom:0em; margin-top:0px; padding-top:0em;">
                <thead>
                    <tr>
                        <th scope="col">Ambiente</th>
                        <th scope="col">Instancia</th>
                        <th scope="col">Cuenta en Servidor</th>
                        <th scope="col">Cuenta en Control de usuarios</th>
                        <th scope="col">Estatus</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var solicitud in @Model.List_Tabla)
                    {
                        <tr class="renglon " style="font-stretch: condensed;">
                            <td id="ambiente">@solicitud.Ambiente</td>
                            <td id="instancia">@solicitud.Instancia</td>
                            <td id="cuenta_r">@solicitud.Cuenta_Real</td>
                            <td id="cuenta_a">@solicitud.Cuenta_Aplic</td>
                            <td id="estatus">@solicitud.Estatus</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</form>
